+		}
+	}
+}
+@media #{$small-only} {
+	#hero{
+		.owl-logo{
+			display: none;
+		}
+	}
+}
+
+
+
+/* Home Demo */
+
+.owl-carousel{
+	.item{
+
+	}
+}
+.home-demo{
+	padding: 2rem 0;
