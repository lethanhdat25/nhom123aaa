+		padding: 1rem 2rem;
+		margin-right: 0.75rem;
+		font-size: 1.250rem;
+		font-weight: 400;
+		background: #FFF;
+		color: $owl-primary-color;
+		border: 1px solid #FFF;
+		@include owl-radius(2px);
+		@include owl-transition(all,200ms);
